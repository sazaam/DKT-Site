			- var ds = data_sections[i] ;

			//- IMPORTANT FIX --> vars access datas as objects 
			//- - if(!!data_section.article && !!data_section.article.variables){
			//- 	- !!fix && fix(data_section.article, data_section.article.variables, 'variables') ;
