doctype html
html.abs.fullW.fullH.context.whiteBG(lang="en", style="overflow-y: auto;")
	head
		meta(http-equiv="Content-Type", content="text/html; charset=utf-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
		meta(name="apple-mobile-web-app-capable", content="yes")
		meta(name="apple-mobile-web-app-status-bar-style", content="black")
		meta(name="viewport", content="width=device-width, initial-scale=.7")
		meta(name="viewport", content="height=device-height, initial-scale=.7")
		
		title= title
		
		
		link(rel="stylesheet", type="text/css", href="/css/all.css")
		
		- // FOR JADE PARSER SERVER FIRST JET
		- var data_sections = JSON.parse(Data.sections.data) ;
		
		- // LATER FOR IN-BROWSER PARSER SERVER FIRST JET
		script.
			(function(){
				var prefix = 'data_' ;
				var Data = !{JSON.stringify(Data)} ;
				for(var s in Data){
					var node = Data[s] ;
					var data = node.data ;
					var name = prefix + node.name ;
					Data[name] = window[name] = JSON.parse(node.data) ;
					
					console.log('DATA >', name , window[name])
				}
				window.Data = Data ;
			})() ;

	body.abs.all.left0.right0.top0.bottom0
		block content
		
		ol#globalnav.flowvisible.navbar.navbar-custom.navbar-fixed-top.RpadXXLg.LpadSm
			- var ss = data_sections ;
			- var l = ss.length ;
			- for(var i = 0 ; i < l ; i ++){
				- var s = ss[i] ;
				- var name = s.name ;
				- var href = '/#/en/' + name + '/' ;
				
				- var cl = name == 'home' ? 'floatL ' + name : 'floatR ' + name ;
				//- EACH MAIN SECTION LINK
				li(id= "gobal_" + name, class= cl)
					a.block.pad.inconsolata(href= href)
						span= name
			- }
