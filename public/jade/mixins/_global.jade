//- LINKS
mixin link(o)
	ul.links(class= o.cl || "")
		li(style='display: inline;')
			a.gowhite.bt.btn-border-slider-white.round.left.dkt.whiteBG(href=o.url)= t(o.prename || 'view')
		li(style='display: inline;')
			a.goblack.bt.btn-border-slider-black.round.right.dktBG(href=o.url)= t(o.name || 'more')

mixin callus(number, cl)
	.call
		h4.sizeXLg.txtL.inlineblock.padXSm.HpadXLg.antraciteBG.round.left.dkt.dktBD(class=cl)= t('call') 
		h4.sizeXLg.txtL.inlineblock.padXSm.HpadXLg.dktBG.round.right.white.dktBD= t(number)


//- MEDIAS
mixin mediasmallslide(media)
	li.round.left.cover.abs.top0.left0.zi10(style='width:100%;height:100%;background:url(' + CDN + media.url + ') no-repeat 50% 50%;background-size:cover;opacity: 0;')
		span.hidden Image


//- LINK AS A BLOCK
mixin linkblock(ch, isPatchwork, linkname)
	- var name = ch.name ;
	- var cleanname = ch.cleanname ;
	- var surname = ch.surname ;
	- var cleanlink = ch.cleanlink ;
	- var medias = ch.post.bracket[1].media ;
	- if(isPatchwork){
		- var isHome = section.name == 'home' ;
		li.work-item.rel.context(class=isHome ? "size1of2" : "size1of4")
			.work-image
				img(src= CDN + medias[0].url , alt='')
			.work-caption(style="font-size:80%")
				.work-descr(class=isHome ? "sizeXLg" : "sizeR")= surname
				h3.work-title.uppercase.Bmar(class=isHome ? "sizeXXLg" : "sizeXLg")= cleanname 
				+link({name:linkname, cl:"dkt", url:cleanlink})
	- } else {
		.work-item.rel.context.fullW.noMQ(class=cl)
			.work-image
				img(src= CDN + medias[0].url , alt='')
			.work-caption(style="font-size:80%")
				.work-descr.ls1= surname
				h3.work-title.uppercase.Bmar.ls1= cleanname
				+link({name:linkname, cl:"dkt", url:cleanlink})
	- }


//- CORE - LOAD INNER SECTIONS OF A PARENT (PRODUCTS CASE)
mixin loadchildrensection(ch, par)
	section.subsection.fullH.whiteBG(id=ch.name, class=ch.name, style="border-top:10px solid #9d1639;border-bottom:10px solid #9d1639;")
		- var productjade = ch.post.template.jade ;
		- var bracket = ch.post.bracket ;
		- var params = p(merge(locals, ch.post.vars.locals), {filename:basedir + ch.name + '.jade', section:par, child_section: ch}) ;
		
		| !{ render(productjade, merge(params, customize(bracket, params) ))}


