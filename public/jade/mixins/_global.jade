										.sizeXSm.white.abs.left0.bottom0.padXXXXLg(class=cl)
											:marked !{md(t(frame.text, locals))}
										.abs.top0.left0.right0.pad(style="margin-top:100px")
											a.block.arr.arrnext.floatR(href="#")
												span.hidden &gt;
											a.block.arr.arrprev.floatL(href="#")
												span.hidden &lt;
											
									.lastUnit.fullH.rounded.context
										.fullW.fullH.cover(style="background:url(" + CDN + media[i].url +") no-repeat left top;background-size:cover")


